    highlightBackground: true
  "ide-haskell":
    stylishHaskellPath: "/Users/pontusnagy/.local/bin/stylish-haskell"
  "ide-haskell-repl":
    defaultRepl: "stack"
    ghciPath: "/Users/pontusnagy/.local/bin/ghc-modi"
    stackPath: "/usr/local/bin/stack"
  "js-hyperclick":
    extensions: [
      ".js"
      ".json"
      ".node"
      ".jsx"
