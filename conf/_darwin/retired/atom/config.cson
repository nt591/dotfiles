  "language-babel":
    autoIndentJSX: true
  "language-elm":
    elmOraclePath: "/Users/pontusnagy/.nvm/versions/node/v6.4.0/bin/elm-oracle"
  linter:
    errorPanelHeight: 86
    ignoreMatchedFiles: "/**/*.min.{js,css}, /**/*.hs"
